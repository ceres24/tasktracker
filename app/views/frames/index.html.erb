<h1>Listing frames</h1>

<table>
  <tr>
    <th>Project</th>
    <th>User</th>
    <th>Name</th>
    <th>Description</th>
    <th>Start date</th>
    <th>End date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @frames.each do |frame| %>
  <tr>
    <td><%= frame.project_id %></td>
    <td><%= frame.user_id %></td>
    <td><%= frame.name %></td>
    <td><%= frame.description %></td>
    <td><%= frame.start_date %></td>
    <td><%= frame.end_date %></td>
    <td><%= link_to 'Show', frame %></td>
    <td><%= link_to 'Edit', edit_frame_path(frame) %></td>
    <td><%= link_to 'Destroy', frame, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Frame', new_frame_path %>

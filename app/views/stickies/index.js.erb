<% if params[:use_template] == 'redesign' %>

  <% if params[:replace] == 'rows' %>
    $("#stickies_table_container").html("<%= escape_javascript(render("stickies_redesign_table")) %>");
  <% else %>
    $("#stickies-or-templates").html("<%= escape_javascript(render("stickies_redesign")) %>");
  <% end %>


  $('[rel~="tooltip"]').tooltip({ trigger: 'hover' });
  activateStickyDraggables();
  initializeCompletionButtons();

<% else %>
  $("#stickies").html("<%= escape_javascript(render("stickies")) %>");
  $("#sticky_tag_selection_container").html("<%= escape_javascript(render("tags/multi_tag_selection")) %>");
<% end %>
